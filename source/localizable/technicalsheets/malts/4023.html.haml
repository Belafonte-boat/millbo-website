          %td.text
            = I18n.t("technicalsheet.malts.product4023.shelf")



  #table
    %table.footable{:cellspacing => "0"}
      %thead
        %tr.odd
          %th{class:"footable-first-column",:"data-toggle"=>"true"}
            = t("technicalsheet.specifics")
          %th{:"data-hide"=>"phone,table"}
          %th{:"data-hide"=>"phone,table"}

      %tbody
        %tr.even
          %td.text
            = t("technicalsheet.umidity")
