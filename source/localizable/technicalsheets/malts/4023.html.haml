        %tr
          %th.text.main{class:"footable-first-column",:"data-toggle"=>"true"}
            = I18n.t("technicalsheet.malts.product4023.title")
          %th.text{class:"footable-last-column", :"data-hide"=>"phone,table" }
      %tbody
        %tr
          %td.text.main
